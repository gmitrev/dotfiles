hi -a CmdWarningMsg		   color: #ff7400 !important; background: #2f2f2f !important; font-weight: normal !important;
"NOTE: buffer colors
hi -a Normal                color: #e0e0e0 !important; background: #2f2f2f !important; font-weight: normal !important;
hi -a CompItem              color: #d0d0d0 !important; text-shadow: none !important; font-weight: normal !important; background-color: inherit !important;
hi -a CompItem:nth-child(2n+1) background-color: inherit !important;
hi -a CompItem[selected]    color: #e0e0e0; background-color: #4f4f4f !important;
hi -a CompDesc				color: #e0e0e0; width: 50%; font-family: Verdana;
hi -a CompIcon				width: 16px; min-width: 16px; display: inline-block; margin-right: 2ex; margin-left: 2px;
hi -a CompLess				text-align: center; height: 0; line-height: .5ex; padding-top: 1ex;
hi -a CompMore				text-align: center; height: .5ex; line-height: .5ex; margin-bottom: -.5ex;
hi -a CompMsg				font-style: italic; margin-left: 16px;
hi -a CompResult			width: 45%; overfloaw: hidden;
hi -a CompTitle			    opacity: 0.5 !important; color: #e0e0e0; background: #4f4f4f;text-shadow: #2f2f2f 3px 3px 3px; font-weight: bold; font-family: Verdana; font-size: 14px; text-align: center;
"hi -a CompTitleSep			height: 10px; background: -moz-linear-gradient(bottom,#2f2f2f,#4f4f4f) !important;
hi -a CompTitleSep			height: 0px !important; background: #2f2f2f !important;
hi -a CompGroup             color:#e0e0e0 !important;font-weight:normal; background: transparent !important;
"hi -a CompMore::after		
"hi -a CompIcon>img
"hi -a CompItem>*
"NOTE: style arrow 
"hi -a CompLess::after       color:red !important;		
"hi -a CompGroup:last-of-type color: red !important;
"hi -a CompGroup:not(:first-of-type)
"NOTE: gradient
hi -a Gradient				height: 1px; margin-bottom: -1px; margin-top: -1px;
hi -a GradientLeft			background-color: #2f2f2f;
hi -a GradientRight		    background-color: #2f2f2f;
"NOTE: link
hi -a Link                      color: #e0e0e0 !important;
hi -a Link:not(:hover)>LinkInfo color: #e0e0e0 !important;
hi -a Message                   color: #e0e0e0 !important;
hi -a Message\ String           color: #e0e0e0 !important;
hi -a LinkInfo		            color: #e0e0e0 !important;

hi -a URL				text-decoration: none !important; color: #e0e0e0; background: inherit; font-weight:normal !important; font-family: Verdana; text-shadow: none !important;
hi -a URL:hover			text-decoration: underline; cursor: pointer;
