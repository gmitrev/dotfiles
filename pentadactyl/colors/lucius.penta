"NOTE: status line color
hi -a StatusLine           color: #e0e0e0 !important; background-color: #4f4f4f !important; font-weight: normal !important; font-family: Verdana,Ubuntu,Georgia; text-shadow: none; font-size: 16px; padding-top: 2px !important; padding-bottom: 2px !important;
hi -a StatusCmdLine        color: #e0e0e0 !important; content: before(">") !important;
hi -a StatusLine[selected] color: #e0e0e0; background: #4f4f4f; font-weight: normal; font-family: Verdana; text-shadow: none; font-size: 18px;
hi -a StatusLineBroken	   color: #e0e0e0; background: #4f4f4f; font-weight: normal; font-family: Verdana; text-shadow: none; font-size: 18px;
hi -a StatusLineSecure	   color: #e0e0e0; background: #4f4f4f; font-weight: normal; font-family: Verdana; text-shadow: none; font-size: 18px;
hi -a Preview              color: gray;
hi -a Title                color: #e0e0e0 !important; background-color: #4F4F4F !important; font-weight: normal !important; font-family: Verdana,Ubuntu,Georgia; text-shadow: none; font-size: 16px;

hi -a BufTabSelected       color: #e0e0e0 !important; background: #0f0f0f !important; font-weight: normal !important;
hi -a CmdCmdLine           color: #e0e0e0 !important; background: #2f2f2f !important; font-weight: normal !important; font-family: Ubuntu, Georgia !important;font-size: 18px; text-shadow: none !important;
hi -a CmdOutput            color: #e0e0e0 !important; background: #2f2f2f !important; font-weight: normal !important; font-family: Ubuntu, Georgia !important;font-size: 18px; text-shadow: none !important;
hi -a CmdNormal            color: #e0e0e0 !important; background: #2f2f2f !important; font-weight: normal !important; font-family: Ubuntu, Georgia !important;font-size: 18px; text-shadow: none !important;
hi -a CmdInput             color: #e0e0e0 !important; background: #2f2f2f !important; font-weight: normal !important; font-family: Ubuntu, Georgia !important;font-size: 18px; text-shadow: none !important;
hi -a CmdModeMsg		   color: #e0e0e0 !important; background: #2f2f2f !important; font-weight: normal !important;
hi -a CmdInfoMsg		   color: #e0e0e0 !important; background: #2f2f2f !important; font-weight: normal !important;
hi -a CmdMoreMsg		   color: #e0e0e0 !important; background: #2f2f2f !important; font-weight: normal !important;
