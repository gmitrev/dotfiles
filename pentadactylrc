"hg7064

loadplugins '\.(js|penta)$'
group user

so ~/.pentadactyl/theme.penta
so ~/.pentadactyl/user.css

set maxitems=12

map -count -modes=n,v <C-h> -builtin <count>H
map -count -modes=n,v <C-j> -builtin <count><C-p>
map -count -modes=n,v <C-k> -builtin <count><C-n>
map -count -modes=n,v j -builtin <count>5j
map -count -modes=n,v k -builtin <count>5k
map -count -modes=n,v <C-l> -builtin <count>L
set hintkeys=qwedf
set hlfind
set titlestring=''

" New
map l gt
" map L :tabmove +1<CR>
map h gT
" map H :tabmove -1<CR>

map D :tabdetach<CR>
map c :tabduplicate<CR>

map <Left> :back<CR>
map <Right> :forward<CR>
map <C-g> :pageinfo<CR>
" vim: set ft=pentadactyl:
